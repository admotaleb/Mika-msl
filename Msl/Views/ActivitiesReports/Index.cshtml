@model IEnumerable<Msl.Models.ActivitiesReport>
@using Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor
@{
    var userInfo = HttpContextAccessor.HttpContext.Session.GetString("roleName");
}
@{
    ViewData["Title"] = "Index";
}

<p>
    <a asp-action="Create">Create New</a>
</p>
@if (userInfo == "Admin")
{
    <form asp-action="Index">
        <div class="row justify-content-md-center">
            <div class="col col-lg-2">
                <input name="FirstDate" type="date" class="form-control" placeholder="First Date">
            </div>
            <div class="col col-lg-2">
                <input name="LastDate" type="date" class="form-control" placeholder="Last Date">
            </div>
            <div class="col col-lg-2">
                <input type="submit" class="btn btn-primary" value="Search">
            </div>
        </div>
    </form>
}

